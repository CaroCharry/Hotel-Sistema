class Cliente:
    def __init__(self, nombre, identificacion, direccion, telefono, email):
        self._nombre = nombre
        self._identificacion = identificacion
        self._direccion = direccion
        self._telefono = telefono
        self._email = email
#definicion de get
    def get_nombre(self):
        return self._nombre
    def get_deltificacion(self):
        return self._identificacion
    def get_direccion(self):
        return self._direccion
    def get_telefono(self):
        return self._telefono
    def get_email(self):
        return self._email
#definicon de los set
    def set_nombre(self, nombre):
        self._nombre = nombre
    def set_identificacion(self, identificacion):
        self._identificacion = identificacion
    def set_direccion(self, direccion):
        self._direccion = direcccion
    def set_telefono(self, telefono):
        self._telefono = telefono
    def set_email(self, email):
        self._email = email



#Clase 1
class reserva:
#Constructor
    def __init__(self,fechaentrada,fechasalida,idreserva):
        self.fechaentrada = fechaentrada
        self.fechasalida = fechasalida
        self.idreserva = idreserva
#Definici贸n de get
    def getfechaentrada(self):
        return self.fechaentrada
    def getfechasalida(self):
        return self.fechasalida
    def getidreserva(self):
        return self.idreserva
#Definici贸n de set
    def setfechaentrada(self,fentrada):
        self.fechaentrada = fentrada 
    def setfechasalida(self,fsalida):
        self.fechasalida = fsalida
    def setidreserva(self,numreserva):
        self.idreserva = numreserva

#Clase 2  
class habitacion:
#Constructor
    def __init__(self,idhabitacion,tipohabitacion,estado,precioxnoche):
        self.idhabitacion = idhabitacion
        self.tipohabitacion = tipohabitacion
        self.estado = estado
        self.precioxnoche = precioxnoche
#Definici贸n de get
    def getidhabitacion(self):
        return self.idhabitacion
    def gettipohabitacion(self):
        return self.tipohabitacion
    def getestado(self):
        return self.estado
    def getprecioxnoche(self):
        return self.precioxnoche
#Definici贸n de set
    def setidhabitacion(self,numhabitacion):
        self.idhabitacion = numhabitacion
    def settipohabitacion(self,tipehabitacion):
        self.tipohabitacion = tipehabitacion
    def setestado(self,estadohab):
        self.estado = estadohab
    def setprecioxnoche(self,precionoche):
        self.precioxnoche = precionoche
